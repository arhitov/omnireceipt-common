{
    "name": "omnireceipt/common",
    "description": "Common components for the Omnireceipt fiscal receipt processing library",
    "version": "0.0.2",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "receipt",
        "fiscal",
        "omnireceipt"
    ],
    "homepage": "https://github.com/omnireceipt/common",
    "authors": [
        {
            "name": "Alexander Arhitov",
            "email": "clgsru@gmail.com",
            "homepage": "https://github.com/arhitov/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnireceipt\\Common\\": "src/Common"
        },
        "classmap": [
            "src/Omnireceipt.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Omnireceipt\\Common\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0",
        "psr/http-message": "*",
        "symfony/http-foundation": "*",
        "php-http/httplug": "*",
        "php-http/discovery": "*",
        "php-http/message-factory": "*",
        "php-http/message": "*",
        "doctrine/collections": "*",
        "nesbot/carbon": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "php-http/guzzle6-adapter": "*"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
