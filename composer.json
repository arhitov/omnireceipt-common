{
    "name": "omnireceipt/common",
    "description": "Common components for the Omnireceipt fiscal receipt processing library",
    "version": "1.4.0",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "receipt",
        "fiscal",
        "omnireceipt"
    ],
    "homepage": "https://github.com/omnireceipt/common",
    "authors": [
        {
            "name": "Alexander Arhitov",
            "email": "clgsru@gmail.com",
            "homepage": "https://github.com/arhitov/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnireceipt\\Common\\": "src/Common"
        },
        "classmap": [
            "src/Omnireceipt.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Omnireceipt\\Common\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0",
        "psr/http-message": "^2",
        "symfony/http-foundation": "^v6",
        "php-http/discovery": "^1.19",
        "doctrine/collections": "^1.8"
    },
    "require-dev": {
        "php-http/curl-client": "^2",
        "guzzlehttp/psr7": "^2.0",
        "phpunit/phpunit": "^9",
        "nesbot/carbon": "^2"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
